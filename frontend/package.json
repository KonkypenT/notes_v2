{
  "name": "events-with-friend",
  "version": "0.0.1",
  "author": "Amsterz",
  "homepage": "http://amsterz.ru/",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open --proxy-config proxy.conf.json --port 4202",
    "start:stage": "ng serve --open --proxy-config proxy-stage.conf.json --port 4203",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prebuild:tst": "cross-env NODE_ENV=stage node prebuild.js",
    "afterbuild:android": "cross-env PLATFORM=android node afterbuild.js",
    "afterbuild:ios": "cross-env PLATFORM=ios node afterbuild.js",
    "build:stage:android:open": "npm run prebuild:tst && ionic capacitor build android -c=stage --device --release  && npm run afterbuild:android",
    "build:stage:ios:open": "npm run prebuild:tst && ionic capacitor build ios -c=stage --device --release && npm run afterbuild:ios"
  },
  "private": true,
  "dependencies": {
    "@2gis/mapgl": "^1.28.0",
    "@angular/common": "^14.0.0",
    "@angular/core": "^14.0.0",
    "@angular/forms": "^14.0.0",
    "@angular/platform-browser": "^14.0.0",
    "@angular/platform-browser-dynamic": "^14.0.0",
    "@angular/router": "^14.0.0",
    "@awesome-cordova-plugins/core": "^5.44.0",
    "@awesome-cordova-plugins/geolocation": "^5.44.0",
    "@capacitor/android": "^4.0.0",
    "@capacitor/app": "^4.0.0",
    "@capacitor/core": "^4.0.0",
    "@capacitor/haptics": "^4.0.0",
    "@capacitor/ios": "^4.0.0",
    "@capacitor/keyboard": "^4.0.0",
    "@capacitor/status-bar": "^4.0.0",
    "@ionic/angular": "^6.2.1",
    "@ngxs/devtools-plugin": "^3.7.4-dev.master-38cbcac",
    "@ngxs/logger-plugin": "^3.7.4-dev.master-38cbcac",
    "@ngxs/store": "^3.7.4-dev.master-38cbcac",
    "cordova-plugin-geolocation": "^4.1.0",
    "cross-env": "^7.0.3",
    "git-repo-info": "^2.1.1",
    "ionic-selectable": "^4.9.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.2.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.0.0",
    "@angular-eslint/builder": "~13.0.1",
